@model IEnumerable<Vidly.Models.Movie>
@{
    ViewBag.Title = "Index";
}

<hr />
<br />

<table class="table  table-striped">
    <tr>
        <th>Movie</th>
        <th>Genre</th>
    </tr>
    @foreach (var movie in Model)
    {
        <tr>
            <td>@Html.ActionLink(movie.Name, "Details", new {id=movie.Id})</td>
            
            <td>@movie.Genre.Name</td>

        </tr>
    }


</table>

